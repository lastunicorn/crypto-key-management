    <!-- Color Definitions -->
    <Color x:Key="BackgroundColor">#FF1E1E1E</Color>
    <Color x:Key="SurfaceColor">#FF2D2D30</Color>
    <Color x:Key="SurfaceHoverColor">#FF3E3E42</Color>
    <Color x:Key="SurfaceDisabledColor">#FF232323</Color>
    <Color x:Key="BorderColor">#FF3F3F46</Color>
    <Color x:Key="BorderDisabledColor">#FF404040</Color>
    <Color x:Key="TextColor">#FFD0D0D0</Color>
    <Color x:Key="TextSecondaryColor">#FFCCCCCC</Color>
    <Color x:Key="TextDisabledColor">#FF999999</Color>
