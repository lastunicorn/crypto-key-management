    <Color x:Key="AccentPressedColor">#FF005A9E</Color>
    <Color x:Key="IconStrokeColor">#FFB0B0B0</Color>

    <!-- Brush Definitions -->
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}"/>
