<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Create key icon -->
    <DrawingImage x:Key="CreateKeyIcon">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <!-- Outer rounded square -->
                <GeometryDrawing Brush="Transparent">
                    <GeometryDrawing.Pen>
                        <Pen Brush="{StaticResource IconStrokeBrush}" Thickness="1.2"/>
                    </GeometryDrawing.Pen>
                    <GeometryDrawing.Geometry>
                        <RectangleGeometry Rect="2,2,12,12" RadiusX="2" RadiusY="2"/>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                <!-- Plus sign -->
                <GeometryDrawing>
                    <GeometryDrawing.Pen>
                        <Pen Brush="{StaticResource IconStrokeBrush}" Thickness="1.2" StartLineCap="Square" EndLineCap="Square"/>
                    </GeometryDrawing.Pen>
                    <GeometryDrawing.Geometry>
                        <GeometryGroup>
                            <LineGeometry StartPoint="8,5" EndPoint="8,11"/>
                            <LineGeometry StartPoint="5,8" EndPoint="11,8"/>
                        </GeometryGroup>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>

</ResourceDictionary>